{
    "$schema": "https://schemas.wp.org/wp/6.6/theme.json",
    "settings": {
        "blocks": {
            "core/button": {
                "appearanceTools": true,
                "border": {
                    "color": false
                }
            },
            "core/buttons": {
                "spacing": {
                    "margin": true
                }
            },
            "core/columns": {
                "spacing": {
                    "blockGap": true
                }
            },
            "core/cover": {
                "appearanceTools": true
            },
            "core/gallery": {
                "color": {
                    "caption": true
                }
            },
            "core/group": {
                "background": {
                    "backgroundImage": true,
                    "backgroundSize": true
                },
                "border": {
                    "color": true,
                    "radius": true,
                    "width": true
                },
                "color": {
                    "background": true
                },
                "spacing": {
                    "margin": true,
                    "padding": true
                }
            },
            "core/heading": {
                "spacing": {
                    "margin": true,
                    "padding": true
                },
                "typography": {
                    "lineHeight": true
                }
            },
            "core/image": {
                "border": {
                    "color": true,
                    "radius": true,
                    "style": true,
                    "width": true
                },
                "dimensions": {
                    "aspectRatio": true
                }
            },
            "core/list": {
                "appearanceTools": true
            },
            "core/media-text": {
                "spacing": {
                    "margin": true,
                    "padding": true
                }
            },
            "core/paragraph": {
                "spacing": {
                    "margin": true,
                    "padding": true
                },
                "typography": {
                    "lineHeight": true
                }
            },
            "core/query": {
                "appearanceTools": true
            }
        },
        "color": {
            "custom": true,
            "defaultDuotone": false,
            "defaultGradients": false,
            "defaultPalette": false,
            "gradients": [
                {
                    "gradient": "linear-gradient(135deg,rgb(48,54,85) 0%,rgb(75,85,132) 100%)",
                    "name": "Standard Blue",
                    "slug": "standard-blue"
                }
            ],
            "palette": [
                {
                    "color": "rgba(0,0,0,1)",
                    "name": "Black",
                    "slug": "black"
                },
                {
                    "color": "rgb(34, 34, 34, 1)",
                    "name": "Dark Gray",
                    "slug": "dark-gray"
                },
                {
                    "color": "rgb(66, 84, 102, 1)",
                    "name": "Slate Gray",
                    "slug": "slate-gray"
                },
                {
                    "color": "rgb(128, 128, 128, 1)",
                    "name": "Medium Gray",
                    "slug": "med-gray"
                },
                {
                    "color": "rgb(193, 193, 193, 1)",
                    "name": "Light Gray",
                    "slug": "light-gray"
                },
                {
                    "color": "rgb(245, 245, 245, 1)",
                    "name": "White",
                    "slug": "white"
                },
                {
                    "color": "rgb(255, 255, 255, 1)",
                    "name": "Full White",
                    "slug": "full-white"
                },
                {
                    "color": "rgb(40, 170, 226, 1)",
                    "name": "Light Blue",
                    "slug": "light-blue"
                },
                {
                    "color": "rgb(10, 37, 64, 1)",
                    "name": "Dark Blue",
                    "slug": "dark-blue"
                },
                {
                    "color": "rgb(1, 115, 230, 1)",
                    "name": "Medium Blue",
                    "slug": "med-blue"
                },
                {
                    "color": "rgb(1, 115, 230, .2)",
                    "name": "Translucent Blue",
                    "slug": "trans-blue"
                },
                {
                    "color": "rgb(102, 41, 95)",
                    "name": "Purple",
                    "slug": "purple"
                },
                {
                    "color": "#303655",
                    "name": "Translucent Purple",
                    "slug": "trans-purple"
                },
                {
                    "color": "#e9ebf3",
                    "name": "Green",
                    "slug": "green"
                },
                {
                    "color": "#e6e3d3",
                    "name": "Translucent Green",
                    "slug": "trans-green"
                },
                {
                    "color": "#f3f1e9",
                    "name": "Orange",
                    "slug": "orange"
                },
                {
                    "color": "#4b5584",
                    "name": "Translucent Orange",
                    "slug": "trans-orange"
                },
                {
                    "color": "#303655",
                    "name": "Martinique",
                    "slug": "custom-martinique"
                },
                {
                    "color": "#e9ebf3",
                    "name": "Martinique light",
                    "slug": "custom-martinique-light"
                },
                {
                    "color": "#e6e3d3",
                    "name": "Thatch green light 1",
                    "slug": "custom-thatch-green-light-1"
                },
                {
                    "color": "#f3f1e9",
                    "name": "Thatch green light 2",
                    "slug": "custom-thatch-green-light-2"
                },
                {
                    "color": "#4b5584",
                    "name": "Martinique card BG",
                    "slug": "custom-martinique-card-bg"
                }
            ]
        },
        "layout": {
            "contentSize": "1280px",
            "wideSize": "1440px"
        },
        "shadow": {
            "defaultPresets": false
        },
        "spacing": {
            "blockGap": true,
            "defaultSpacingSizes": false,
            "spacingSizes": [
                {
                    "name": "3X-Small",
                    "size": ".25rem",
                    "slug": "xxxsmall"
                },
                {
                    "name": "2X-Small",
                    "size": ".5rem",
                    "slug": "xxsmall"
                },
                {
                    "name": "X-Small",
                    "size": ".75rem",
                    "slug": "xsmall"
                },
                {
                    "name": "Small",
                    "size": "1rem",
                    "slug": "small"
                },
                {
                    "name": "Medium",
                    "size": "1.5rem",
                    "slug": "medium"
                },
                {
                    "name": "Large",
                    "size": "2rem",
                    "slug": "large"
                },
                {
                    "name": "X-Large",
                    "size": "3rem",
                    "slug": "xlarge"
                },
                {
                    "name": "2X-Large",
                    "size": "4rem",
                    "slug": "xxlarge"
                },
                {
                    "name": "3X-Large",
                    "size": "6rem",
                    "slug": "xxxlarge"
                },
                {
                    "name": "Global Inline",
                    "size": "var(--global-inline-padding)",
                    "slug": "global-inline"
                }
            ],
            "units": ["rem", "em"]
        },
        "typography": {
            "defaultFontSizes": false,
            "fontFamilies": [
                {
                    "fontFace": [
                        {
                            "fontFamily": "Poppins",
                            "fontStyle": "normal",
                            "fontWeight": "700",
                            "src": [
                                "file:./assets/fonts/poppins/Poppins-Bold.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Poppins",
                            "fontStyle": "normal",
                            "fontWeight": "500",
                            "src": [
                                "file:./assets/fonts/poppins/Poppins-Medium.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Poppins",
                            "fontStyle": "normal",
                            "fontWeight": "400",
                            "src": [
                                "file:./assets/fonts/poppins/Poppins-Regular.ttf"
                            ]
                        }
                    ],
                    "fontFamily": "\"Poppins\", sans-serif",
                    "name": "Poppins",
                    "slug": "heading"
                },
                {
                    "fontFace": [
                        {
                            "fontFamily": "IBM Plex Sans",
                            "fontStyle": "normal",
                            "fontWeight": "700",
                            "src": [
                                "file:./assets/fonts/ibm-plex/IBMPlexSans-Bold.woff2"
                            ]
                        },
                        {
                            "fontFamily": "IBM Plex Sans",
                            "fontStyle": "italic",
                            "fontWeight": "700",
                            "src": [
                                "file:./assets/fonts/ibm-plex/IBMPlexSans-BoldItalic.woff2"
                            ]
                        },
                        {
                            "fontFamily": "IBM Plex Sans",
                            "fontStyle": "normal",
                            "fontWeight": "500",
                            "src": [
                                "file:./assets/fonts/ibm-plex/IBMPlexSans-Regular.woff2"
                            ]
                        },
                        {
                            "fontFamily": "IBM Plex Sans",
                            "fontStyle": "italic",
                            "fontWeight": "500",
                            "src": [
                                "file:./assets/fonts/ibm-plex/IBMPlexSans-Italic.woff2"
                            ]
                        }
                    ],
                    "fontFamily": "\"IBM Plex Sans\", sans-serif",
                    "name": "IBM Plex",
                    "slug": "body"
                }
            ],
            "fontSizes": [
                {
                    "name": "X-Small",
                    "size": "var(--font-size-xs)",
                    "slug": "xsmall"
                },
                {
                    "name": "Small",
                    "size": "var(--font-size-sm)",
                    "slug": "small"
                },
                {
                    "name": "Base",
                    "size": "var(--font-size-base)",
                    "slug": "base"
                },
                {
                    "name": "H6",
                    "size": "var(--font-size-base)",
                    "slug": "h6"
                },
                {
                    "name": "H5",
                    "size": "var(--font-size-base)",
                    "slug": "h5"
                },
                {
                    "name": "H4",
                    "size": "var(--font-size-lg)",
                    "slug": "h4"
                },
                {
                    "name": "H3",
                    "size": "var(--font-size-xl)",
                    "slug": "h3"
                },
                {
                    "name": "H2",
                    "size": "var(--font-size-2xl)",
                    "slug": "h2"
                },
                {
                    "name": "H1",
                    "size": "var(--font-size-3xl)",
                    "slug": "h1"
                }
            ],
            "lineHeight": true
        }
    },
    "styles": {
        "blocks": {
            "core/button": {
                "border": {
                    "bottom": {
                        "color": "var(--wp--preset--color--full-white)",
                        "style": "solid",
                        "width": "1px"
                    },
                    "left": {
                        "color": "var(--wp--preset--color--full-white)",
                        "style": "solid",
                        "width": "1px"
                    },
                    "radius": "0.25rem",
                    "right": {
                        "color": "var(--wp--preset--color--full-white)",
                        "style": "solid",
                        "width": "1px"
                    },
                    "top": {
                        "color": "var(--wp--preset--color--full-white)",
                        "style": "solid",
                        "width": "1px"
                    }
                },
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--body)",
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "core/column": {
                "spacing": {
                    "padding": {
                        "bottom": "0",
                        "left": "0",
                        "right": "0",
                        "top": "0"
                    }
                }
            },
            "core/columns": {
                "spacing": {
                    "blockGap": "0",
                    "padding": {
                        "bottom": "0",
                        "left": "0",
                        "right": "0",
                        "top": "0"
                    }
                }
            },
            "core/group": {
                "spacing": {
                    "blockGap": "0",
                    "margin": {
                        "bottom": "0",
                        "top": "0"
                    },
                    "padding": {
                        "bottom": "0",
                        "left": "var(--global-inline-padding)",
                        "right": "var(--global-inline-padding)",
                        "top": "0"
                    }
                }
            },
            "core/heading": {
                "spacing": {
                    "margin": {
                        "bottom": "0",
                        "left": "0",
                        "right": "0",
                        "top": "0"
                    },
                    "padding": {
                        "bottom": "0",
                        "left": "0",
                        "right": "0",
                        "top": "0"
                    }
                },
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading)"
                }
            },
            "core/home-link": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--body)",
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "core/image": {
                "spacing": {
                    "margin": {
                        "bottom": "0"
                    }
                }
            },
            "core/navigation": {
                "spacing": {
                    "blockGap": "var(--wp--preset--spacing--medium)"
                },
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--body)",
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontStyle": "normal",
                    "fontWeight": "700"
                }
            },
            "core/navigation-link": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--body)",
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "core/site-title": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading)",
                    "fontStyle": "normal",
                    "fontWeight": "400"
                }
            }
        },
        "color": {
            "text": "var(--wp--preset--color--dark-blue)"
        },
        "elements": {
            "button": {
                "typography": {
                    "fontStyle": "normal",
                    "fontWeight": "700"
                }
            }
        },
        "spacing": {
            "blockGap": "24px"
        },
        "typography": {
            "fontFamily": "Poppins"
        }
    },
    "title": "Creative Homeworks",
    "version": 3
}
